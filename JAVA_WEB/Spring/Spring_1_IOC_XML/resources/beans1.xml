<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 创建一个对象 id为一对应一个类，可以在程序中生成这个类 class 为类的路径 -->
	<bean id="student" class="model.Student"></bean>

	<!-- 添加属性 Property name和set方法的name对应 -->
	<bean id="student1" class="model.Student">
		<property name="name" value="学生1">
		</property>

	</bean>

	<bean id="student2" class="model.Student">
		<property name="name" value="学生2">
		</property>
	</bean>

	<!-- 添加属性 Property name和set方法的name对应 -->
	<bean id="teacher" class="model.Teacher">
		<property name="name" value="老师1">
		</property>

		<property name="myStudents">
			<list>
				<ref bean="student1" />
				<ref bean="student2" />
			</list>
		</property>

	</bean>



</beans>






